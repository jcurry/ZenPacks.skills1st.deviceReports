<script type="text/javascript">
Ext.onReady(function(){
var viewport = new Ext.Viewport({
    id: 'viewport',
    layout: 'border',
    cls: 'zenui3',
    defaults: {
        'border': false
    },
    items: [
        new Ext.BoxComponent({
            region: 'north',
            el: 'header',
            height: 75,
            id: 'header_panel'
        }),{
            id: 'footer_panel',
            region: 'south',
            height: 30,
            layout: 'fit',
            el: 'footer',
            items: [
                new Ext.Toolbar({
                    border: false,
                    id: 'footer_bar',
                    listeners: {
                        render: function(bar){
                            Ext.get('footer_extra').appendTo(bar.getEl());
                        }
                    }
                })
            ],
            border: false
        },{
            id: 'center_panel',
            region: 'center',
            el: 'center',
            layout: 'fit',
            border: false,
            defaults: {
                'border': false
            }
        }
    ]
});
});
</script>


